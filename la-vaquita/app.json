{
  "expo": {
    "name": "La Vaquita",
    "slug": "la-vaquita",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-vaquita.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A008B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.javito.lavaquita"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#4A008B"
      },
      "package": "com.javito.lavaquita"
    },
    "extra": {
      "eas": {
        "projectId": "1deb6b1f-836c-43c4-a123-6257b831b948"
      }
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store"
    ]
  }
}